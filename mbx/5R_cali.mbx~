// ibexsolve 5R.mbx
// ibexsolve 5R.mbx
constants
constants


    // architecture parameters
    // architecture parameters
    L1 = 
    L1 = 
    // pose parameters
    // pose parameters
	x1 = 5.0;
    x1 = 5;
	x2 = -19.999999999999975;
    x2 = 5;


variables 
variables 


    // command parameters
    // command parameters
    q1 in [-180,180];
    q1 in [-180,180];
    q2 in [0,360];
    q2 in [0,360];


constraints
constraints


    // kinematic equations
    // kinematic equations
    -l3^2 + (L1 + x1 - l1*cos(q1*pi/180))^2 + (x2 - l1*sin(q1*pi/180))^2 = 0;
    -l3^2 + (L1 + x1 - l1*cos(q1*pi/180))^2 + (x2 - l1*sin(q1*pi/180))^2 = 0;
    -l4^2 + (L2 + x1 - l2*cos(q2*pi/180))^2 + (x2 - l2*sin(q2*pi/180))^2 = 0;
    -l4^2 + (L2 + x1 - l2*cos(q2*pi/180))^2 + (x2 - l2*sin(q2*pi/180))^2 = 0;


end
end
